{
  "name": "ai-plugin-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n plugin,next 'npm run build:plugin -- --watch' 'next dev'",
    "start": "next start",
    "build": "concurrently -n plugin,next 'npm run build:plugin' 'next build'",
    "lint": "next lint",
    "build:plugin": "esbuild plugin/code.ts --bundle --target=es6 --loader:.svg=text --outfile=plugin/dist/code.js",
    "build:next": "next build"
  },
  "dependencies": {
    "@figma/plugin-typings": "*",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "ai": "^2.1.20",
    "autoprefixer": "10.4.14",
    "esbuild": "^0.18.12",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.10",
    "next": "13.4.10",
    "openai-edge": "^1.2.0",
    "postcss": "8.4.26",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
